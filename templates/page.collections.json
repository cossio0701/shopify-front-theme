{
  "sections": {
    "breadcrumbs": {
      "type": "breadcrumbs",
      "settings": {
        "alignment": "left",
        "color_scheme": "background-1",
        "section_width": "wrapper",
        "spacing_top": 1,
        "spacing_bottom": 1
      }
    },
    "page_title": {
      "type": "page-title",
      "settings": {
        "title": "{{ page.title }}",
        "heading_tag": "h1",
        "heading_size": "h0",
        "alignment": "left",
        "bold": true,
        "uppercase": false,
        "color_scheme": "background-1",
        "section_width": "wrapper",
        "spacing_top": 1,
        "spacing_bottom": 2
      }
    },
    "main": {
      "type": "main-page",
      "settings": {
        "breadcrumbs": false,
        "show_page_title": false,
        "page_title_alignment": "text-center",
        "info_title": "",
        "info_content": "",
        "section_width": "max-w-page",
        "color_scheme": "",
        "spacing_top": "0",
        "spacing_bottom": "2"
      }
    }
  },
  "order": [
    "breadcrumbs",
    "page_title",
    "main"
  ]
}
